    "dependencies": {
        "chai": "1.9.1",
        "form-data": "0.1.4",
        "iconv-lite": "0.4.4",
        "performance-now": "0.1.3",
        "request": "2.40.0",
        "soap": "0.8.0",
        "underscore": "1.6.0",
        "urlencode": "0.2.0",
        "yog-log": "0.0.10"
    },
    "devDependencies": {
        "async": "0.9.0",
